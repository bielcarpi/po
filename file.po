var a = 0

func multiplicarNumeros(num, b) {
    num--

    var i
    var j
    for i = 0, i < 10, i++ {
        for j = 0, j < 10, j++ {
            b = i * j
            while i == 0 {
                continue
                break
            }
        }
    }

    ret b
}

main() {
    a = multiplicarNumeros(8, 0)
    print(a)
    for a = 0, a < 10, a++ {
        break
        //you can use continue too
    }
}
