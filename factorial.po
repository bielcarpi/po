func main() {
    var recursive = 0; //Either iterative (0) or recursive(1)
    var n = 10 //Num of terms
    var firstTerm = 0
    var secondTerm = 0
    print("Fibonacci Series till 10 terms:")

    var i
    for i = 0, i <= n, i++ {
        print(firstTerm)

        //Compute the next term
        var nextTerm = firstTerm + secondTerm
        firstTerm = secondTerm
        secondTerm = nextTerm
    }
}

