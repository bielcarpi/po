var num1 = 5
var num2 = 2
var option = 2

main() {
    var aux

    switch option {
        opt 0 -> {
            print(num2)
        }
        opt 1 -> {
            print(num1)
        }
        opt 2 -> {
            aux = num1 + num2
            print(aux)
        }
        default -> {
            prints("Invalid option")
        }
    }
}