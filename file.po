var a = 0

func multiplicarNumeros(num, b) {
    num--

    if num >= 1 {
        b = multiplicarNumeros(num, 0)
        b = b * num
    }
    else {
        ret 1
    }

    ret b
}

main() {
    a = multiplicarNumeros(8, 0)
    print(a)
    for a = 0, a < 10, a++ {
        break
        //you can use continue too
    }
}
