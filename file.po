var a = 0

func multiplicarNumeros(num, b) {
    num--

    if num >= 1 {
        b = b * num
        if num == 0{
            ret 1
        }
        else{
            b++
        }
        num++
    }
    else {
        num++
        ret 1
    }

    ret b
}

main() {
    a = multiplicarNumeros(8, 0)
    print(a)
    for a = 0, a < 10, a++ {
        break
        //you can use continue too
    }
}
